<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@id/navigation_search">

    <fragment
        android:id="@+id/navigation_profile"
        android:name="com.rexhaj.ticketfindernew.ui.profile.ProfileFragment"
        android:label="Profile"
        tools:layout="@layout/fragment_search" />

    <fragment
        android:id="@+id/navigation_search"
        android:name="com.rexhaj.ticketfindernew.ui.search.SearchFragment"
        android:label="Search"
        tools:layout="@layout/fragment_profile" />

    <fragment
        android:id="@+id/navigation_favorites"
        android:name="com.rexhaj.ticketfindernew.ui.favorites.FavoritesFragment"
        android:label="Favorites"
        tools:layout="@layout/fragment_favorites" />
</navigation>